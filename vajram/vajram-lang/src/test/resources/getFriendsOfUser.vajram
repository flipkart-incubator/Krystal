public set<string> getFriendsOfUser(mod string userId, app FriendServiceClient svcClient){
  FriendIdsResponse friendIdsResponse = {
      return svcClient.getFriendsIds(
          FriendServiceClient.makeFriendIdsRequest(
              this._mod.stream()
                  .map(_.userId())
                  .toSet()
          )
      );
    }

  {
      return friendIdsResponse
          .stream()
          .map({
            return this.Mod(_.userId()), _.friendIds();
          });
    }
}